	}
	$npane2++;
	$rolling_item2.css('left','-'+( ($npane2-1) * 222*$page_block )+'px');
});

$rolling_item3   =  $('#rolling_item3');
$rolling_left3   =  $('#rolling_left3');
$rolling_right3  =  $('#rolling_right3');
$rolling_length3 = $rolling_item2.find('.item').length;
$npane3          = 1;
$tpage3          = Math.ceil($rolling_length3/$page_block);

$rolling_left3.click(function(e){
	e.preventDefault();
	if( ($npane3-1) < 1 ){
		alert('첫 페이지 입니다.');
		return false;
	}
	$npane3--;
	$rolling_item3.css('left','-'+( ($npane3-1) * 222*$page_block )+'px');
});

$rolling_right3.click(function(e){
	e.preventDefault();
	if( ($npane3+1) > $tpage3 ){
		alert('마지막 페이지 입니다.');
		return false;
	}
	$npane3++;
	$rolling_item3.css('left','-'+( ($npane3-1) * 222*$page_block )+'px');
});


$('.roll_area').find('img').each(function(){
	$(this).css({ 'margin-top' : '-' + $(this).height()/2+'px' , 'margin-left' : '-' + $(this).width()/2+'px' });
	$(this).load(function(){
		$(this).css({ 'margin-top' : '-' + $(this).height()/2+'px' , 'margin-left' : '-' + $(this).width()/2+'px' });
	});
});


