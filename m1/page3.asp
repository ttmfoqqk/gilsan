<SCRIPT type="text/javascript">
$btn = $('.btn_area button');
$map = $('.map_cont');

$map.not(':first').hide();

$btn.click(function(){
	$btn.removeClass('on');
	$(this).addClass('on');
	
	var _index = $(this).index();
	
	$map.hide();
	$map.filter(':eq('+ _index +')').show();

	$('.roll_area').find('img').each(function(){
		$(this).css({ 'margin-top' : '-' + $(this).height()/2+'px' , 'margin-left' : '-' + $(this).width()/2+'px' });
		$(this).load(function(){
			$(this).css({ 'margin-top' : '-' + $(this).height()/2+'px' , 'margin-left' : '-' + $(this).width()/2+'px' });
		});
	});

});


$page_block      = 3;


$rolling_item1   =  $('#rolling_item1');
$rolling_left1   =  $('#rolling_left1');
$rolling_right1  =  $('#rolling_right1');
$rolling_length1 = $rolling_item1.find('.item').length;
$npane1          = 1;
$tpage1          = Math.ceil($rolling_length1/$page_block);

$rolling_left1.click(function(e){
	e.preventDefault();
	if( ($npane1-1) < 1 ){
		alert('첫 페이지 입니다.');
		return false;
	}
	$npane1--;
	$rolling_item1.css('left','-'+( ($npane1-1) * 222*$page_block )+'px');
});

$rolling_right1.click(function(e){
	e.preventDefault();
	if( ($npane1+1) > $tpage1 ){
		alert('마지막 페이지 입니다.');
		return false;
	}
	$npane1++;
	$rolling_item1.css('left','-'+( ($npane1-1) * 222*$page_block )+'px');
});

